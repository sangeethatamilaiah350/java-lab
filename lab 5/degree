package com.mycompany.red;

/**
 *
 * @author SANGEETHA
 */
//    -----------------METHOD OVERRIDING----------------
public class files {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        degree obj1=new degree();
        System.out.println("object of DEGREE");
        obj1.getdegree();
        System.out.println("object of UNDERGRADUATE");
        underGraduate obj2=new underGraduate();
        obj2.getdegree();
        System.out.println("object of POSTGRADUATE");
        postGraduate obj3=new postGraduate();
        obj3.getdegree();
    }
    
}
class degree{
    void  getdegree(){
        System.out.println("I GOT A DEGREE");
    }
}
class underGraduate extends degree{
    void getdegree(){
        System.out.println("I'M A UNDERGRADUATE");
    }
}
class postGraduate extends degree{
    void getdegree(){
        System.out.println("I'M A POSTGRADUATE");
    }
}
